/* component/banner/banner.wxss */
.banner{
  width: 750rpx;
  height: 165rpx;
  position: relative;
}
.banner .title{
  position: absolute;
  display: block;
  text-align: center;
  width: 100%;
  color: #fff;
  height:100%;
  line-height:165rpx;
  font-size:36rpx;
}
.banner .image{
  position: absolute;
  width: 100%;
  height: 100%;
}